{
    "name": "rhyslees/laravel-trello",
    "description": "Trello integration for Laravel 8",
    "license": "MIT",
    "authors": [
        {
            "name": "Rhys Lees",
            "email": "43909932+RhysLees@users.noreply.github.com"
        }
